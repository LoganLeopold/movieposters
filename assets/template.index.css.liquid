    visibility: hidden;
    width: calc(100% + 16px);
    position: relative;
    left: -8px;
}
.hero.hero--slider.flickity-enabled {
    visibility: visible;
}
.hero.hero--slider .flickity-slider {
    transform: none !important;
}
.hero.hero--slider .flickity-viewport {
    overflow: visible;
}
.hero__slide {
    height: 448px;
    width: 100%;
    left: 0 !important;
    background-size: cover;
    background-position: 50%;
    background-repeat: no-repeat;
    z-index: -1;
    opacity: 0;
    transition: all 0.25s cubic-bezier(0.46, 0.01, 0.32, 1);
}
